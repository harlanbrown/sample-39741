<?xml version="1.0"?>
<component name="org.nuxeo.sample.listener">
    <extension target="org.nuxeo.ecm.platform.audit.service.NXAuditEventsService" point="event">
        <event name="myEvent" />
    </extension>
    <extension target="org.nuxeo.ecm.core.event.EventServiceComponent" point="listener">
        <listener async="true" postCommit="true" name="samplelistener" class="org.nuxeo.sample.SampleListener">
            <event>myEvent</event>
        </listener>
    </extension>
</component>
